{
  "name": "scourjs",
  "description": "Traverse objects and arrays",
  "version": "1.0.1",
  "author": "Rico Sta. Cruz <rico@ricostacruz.com>",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "bugs": {
    "url": "https://github.com/rstacruz/scour/issues"
  },
  "dependencies": {
    "object-assign": "4.0.1",
    "scour-search": "0.3.3",
    "to-function": "2.0.6"
  },
  "devDependencies": {
    "babel-cli": "6.6.4",
    "babel-preset-es2015": "6.16.0",
    "docpress": "0.6.13",
    "expect": "1.20.2",
    "git-update-ghpages": "1.3.0",
    "istanbul": "0.4.2",
    "mdx": "0.3.1",
    "sinon": "2.3.2",
    "sinon-in-sandbox": "1.0.0",
    "standard": "6.0.5",
    "tap-spec": "4.1.1",
    "tape": "4.5.0",
    "uglifyjs": "2.4.10"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/rstacruz/scour#readme",
  "jsnext:main": "src/index.js",
  "keywords": [
    "array",
    "immutable",
    "object"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rstacruz/scour.git"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "coverage": "istanbul cover test/index.js",
    "prepublish": "make -B update && npm run build",
    "test": "node test | tap-spec",
    "test:lint": "standard src/* test/* utilities/*",
    "watch": "babel -w src --out-dir lib"
  }
}
